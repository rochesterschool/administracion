<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollo.data.InscPersonaGrupoFamiliar" table="insc_persona_grupo_familiar" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idInscPersonaGrupoFamiliar" type="integer">
            <column name="id_Insc_Persona_Grupo_Familiar"/>
            <generator class="identity"/>
        </id>
        <property name="responsable" type="boolean">
            <column name="responsable" not-null="true"/>
        </property>
        <many-to-one name="persona" class="com.aprendoz_desarrollo.data.Persona" cascade="none">
            <column name="Persona_Id_Persona" not-null="true"/>
        </many-to-one>
        <many-to-one name="tipoPersona" class="com.aprendoz_desarrollo.data.TipoPersona" cascade="none">
            <column name="Tipo_Persona_Id_Tipo_Persona" not-null="true"/>
        </many-to-one>
        <many-to-one name="grupoFamiliar" class="com.aprendoz_desarrollo.data.GrupoFamiliar" cascade="none">
            <column name="Grupo_familiar_Id_Grupo_Familiar" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>