<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollo.data.Ee" table="ee" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.aprendoz_desarrollo.data.EeId">
            <key-property name="lala" type="integer">
                <column name="lala"/>
            </key-property>
            <key-property name="pepe" type="string">
                <column name="pepe" length="65535"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>